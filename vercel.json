{
  "crons": [
    {
      "path": "/api/cron/post",
      "schedule": "0 12 * * *"
    }
  ],
  "headers": [
    {
      "source": "/api/cron/post",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["iad1"]
}

